DirectoryIndex /GPTMS/api/signup/SignupController.php 

Options +FollowSymlinks -MultiViews
RewriteEngine On
RewriteBase /
# Next condition is met when the requested file doesn't exist
RewriteCond %{REQUEST_FILENAME} !-f
# If previous condition was met, use the next rule
RewriteRule ^(.*)$         /GPTMS/api/signup/SignupController.php [L,NC]