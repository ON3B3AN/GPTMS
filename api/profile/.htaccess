DirectoryIndex /GPTMS/api/profile/ProfileController.php 

Options +FollowSymlinks -MultiViews
RewriteEngine On
RewriteBase /
# Next condition is met when the requested file doesn't exist
RewriteCond %{REQUEST_FILENAME} !-f
# If previous condition was met, use the next rule
RewriteRule ^(.*)$         /GPTMS/api/profile/ProfileController.php [L,NC]